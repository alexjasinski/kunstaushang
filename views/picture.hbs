

<div class="picture-container">


  {{#each picture}}
    <h1>{{title}}</h1>
    <hr />
    <div class="picture">
      <img src="{{imageUrl}}" alt="{{description}}" />
      <p>Author: {{author}}</p>
      <p>Description: {{description}}</p>
      <p>Rating: {{rating}}/10</p>
    </div>
    <hr />

  {{/each}}
  <a href="/">Back</a>
</div>